a = 0
b = 0

fn inc_a() {
  a1 = 0
  for i = 0 to 200000000
    a1 += 1
  a = a1
}

fn inc_b() {
  b1 = 0
  for i = 0 to 200000000
    b1 += 1
  b = b1
}

t = ticks()

t1 = thread_create(inc_a)
t2 = thread_create(inc_b)

thread_start(t1)
thread_start(t2)

thread_wait(t1)
thread_wait(t2)

writeln('a = ', a, ', b = ', b)
writeln('Finished in ', ticks() - t, ' ms.')