chars = ' .,:;=|#[]+-:,.'

for Im = -1.2 to 1.2 step 0.05 {
  for Re = -2 to 1 step 0.03 {
    Zr = Re
    Zi = Im
    for n = 0 to length(chars) - 1 {
      Zr2 = Zr * Zr
      Zi2 = Zi * Zi
      if (Zr2 + Zi2) > 4 {
        break
      }
      Zi = (2 * Zr * Zi) + Im
      Zr = (Zr2 - Zi2) + Re
    }
    write(chars[n])
  }
  write('\n')
}